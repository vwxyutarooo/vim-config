{
  "coc.preferences.jumpCommand": "tab drop",
  "diagnostic-languageserver.filetypes": {
    "css": "stylelint",
    "javascript": "stylelint"
  },
  "diagnostic-languageserver.linters": {
    "stylelint": {
      "args": [
        "--formatter",
        "unix",
        "--stdin-filename",
        "%filename"
      ]
    }
  },
  "tsserver.disableAutomaticTypeAcquisition": true,
  "suggest.snippetsSupport": false,
  "suggest.languageSourcePriority": 1,
  "emmet.priority": 99
}
