{
  "coc.preferences.jumpCommand": "tab drop",
  "diagnostic-languageserver.filetypes": {
    "css": "stylelint",
    "javascript": "stylelint"
  },
  "diagnostic-languageserver.linters": {
    "stylelint": {
      "args": [
        "--formatter",
        "unix",
        "--stdin-filename",
        "%filename"
      ]
    }
  }
}
